@{
    if (TempData["SuccessCartMessage"] != null)
    {
        <script src="~/admin/js/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
        <script type="text/javascript">
            toastr.success('@TempData["SuccessCartMessage"]');
        </script>
    }
    

    if (TempData["ErrorCartMessage"] != null)
    {
        <script src="~/admin/js/jquery.min.js"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
        <script type="text/javascript">
            toastr.error('@TempData["ErrorCartMessage"]');
        </script>
    }
}

